<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent" >


    <TextView
        android:id="@+id/time1"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/Status1"

        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="25dp"
        android:layout_marginLeft="-2dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="110dp"
        android:layout_marginBottom="0dp"

        android:focusable="false"
        android:focusableInTouchMode="false"
        android:paddingBottom="10dp"
        android:textColor="#B2524B4B"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/OrderNo_admin"
        android:layout_width="59dp"
        android:layout_height="28dp"

        android:layout_alignParentTop="true"
        android:layout_marginLeft="94dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="92dp"
        android:layout_marginBottom="57dp"
        android:layout_toStartOf="@+id/userid"

        android:focusable="false"
        android:focusableInTouchMode="false"
        android:paddingBottom="10dp"
        android:textColor="#000000"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="90dp"
        android:layout_height="29dp"

        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="31dp"
        android:layout_marginLeft="6dp"
        android:layout_marginTop="9dp"
        android:layout_marginEnd="162dp"
        android:layout_marginBottom="63dp"
        android:layout_toStartOf="@+id/userid"

        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="Order No."
        android:textAlignment="viewStart" />

    <TextView
        android:id="@+id/Status1"
        android:layout_width="113dp"
        android:layout_height="26dp"
        android:layout_alignEnd="@+id/OrderNo_admin"

        android:layout_alignParentTop="true"
        android:layout_marginLeft="91dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="-53dp"
        android:layout_marginBottom="32dp"

        android:focusable="false"
        android:focusableInTouchMode="false"
        android:paddingBottom="10dp"
        android:textColor="#000000"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="84dp"
        android:layout_height="wrap_content"

        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="29dp"
        android:layout_marginLeft="6dp"
        android:layout_marginTop="41dp"
        android:layout_marginEnd="17dp"
        android:layout_marginBottom="36dp"
        android:layout_toStartOf="@+id/Status1"

        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="Address"
        android:textAlignment="viewStart" />

    <TextView
        android:id="@+id/userid"
        android:layout_width="82dp"
        android:layout_height="26dp"
        android:layout_alignBottom="@+id/Status1"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="45dp"
        android:layout_marginBottom="27dp"
        android:focusable="false"
        android:focusableInTouchMode="false"
        android:text="User:"
        android:textAlignment="viewStart" />

    <TextView
        android:id="@+id/price_txt"
        android:layout_width="82dp"
        android:layout_height="26dp"
        android:layout_alignBottom="@+id/Status1"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="49dp"
        android:layout_marginEnd="46dp"
        android:layout_marginBottom="-10dp"
        android:text=""
        android:textAlignment="viewStart"
        android:focusable="false"
        android:focusableInTouchMode="false"/>


</RelativeLayout>